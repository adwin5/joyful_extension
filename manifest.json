{
  "name": "joyful extension",
  "version": "1.2",
  "description": "pre-download and store the given websites in users computer",
  "permissions": [
    "history",
    "downloads", "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },  
  "browser_action": {
    "default_icon": "clock.png"
  },
  "manifest_version": 2
}
